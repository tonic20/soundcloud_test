<html>
  <body>
    <script type='text/javascript' charset='utf-8'>
      var loc = document.location;
      with(window.parent) {
        setTimeout(function() {
          window.eval('<%= escape_javascript(yield()) %>');
          window.loc && loc.replace('about:blank');
        }, 1)
      }
    </script>
  </body>
</html>

